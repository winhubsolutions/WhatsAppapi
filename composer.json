{
  "name": "yocto/whatsapp-business-sdk-php",
  "type": "library",
  "description": "This SDK enables your project to access the WhatsApp Business API in PHP.",
  "keywords":[
    "api",
    "business",
	"composer",
    "json",
    "sdk",
	"php",
    "whatsapp",
	"yocto"
  ],
  "license": "GPL-3.0-or-later",
  "require": {
	"php": "^7.1||^8"
  },
  "require-dev": {
	"phpunit/phpunit": "^7||^8||^9"
  },
  "autoload": {
	"psr-4": {
	  "WHATSAPP\\SDK\\": "src/"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "WHATSAPP\\SDK\\Tests\\": "tests/"
	}
  }
}